{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@beta-lyfe/api",
	"packageManager": "pnpm@9.15.3",
	"exports": {
		".": "./src/index.ts",
		"./client": "./src/api/client.ts",
		"./types": "./src/api/schema.ts",
		"./zod": "./schema.ts"
	},
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"typegen": "openapi-typescript ../docs-api/src/spec/openapi.yaml -o ./src/api/schema.ts",
		"generate:schema": "openapi-zod-client ../docs-api/src/spec/openapi.yaml --output ./schema.ts --export-schemas --no-with-alias",
		"lint": "tsc"
	},
	"dependencies": {
		"@beta-lyfe/api": "file:",
		"@zodios/core": "^10.9.6",
		"openapi-fetch": "^0.13.4",
		"openapi-react-query": "^0.2.9"
	},
	"devDependencies": {
		"openapi-typescript": "^7.4.1",
		"openapi-zod-client": "^1.18.3"
	}
}
