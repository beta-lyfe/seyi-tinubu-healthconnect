{
	"$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"jsx": "react-jsx",
		"composite": true,
		"skipLibCheck": true,
		"module": "ESNext",
		"strict": true,
		"noEmit": true,
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"vite.config.ts",
		"src"
	]
}
